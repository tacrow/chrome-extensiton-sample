{
  "name": "chrome-extension-sample",
  "manifest_version": 2,
  "version": "1.0",
  "description": "chrome extension sample",
  "browser_action": {
    "default_title": "chrome-extension-sample",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "js": ["content_script.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ]
}